<div class="row">
  <div class="col-md-4 col-md-offset-4 center">
    <%= gravatar_for @usuario, size: 150 %>
  </div>
</div>
<h4 align="center"> Artículos de <%= @usuario.username %> </h4>

<%= render 'articles/article', obj: @usuario.articles %>